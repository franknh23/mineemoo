                <type>skin_js</type>
                <name>tm/firecheckout/js/integration/ced_csmultishipping.js</name>
            </action>
        </reference>
        <reference name="checkout.onepage.shipping_method">
            <block type="csmultishipping/onepage_shipping_method_available" name="checkout.onepage.shipping_method.available" as="available">
