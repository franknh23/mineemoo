<config>  
    <tabs>
        <advanced-checkout translate="label">
            <label>AdvancedCheckout.com</label>
            <sort_order>268</sort_order>
        </advanced-checkout>
    </tabs> 
    <sections>
        <onestepcheckout translate="label" module="certificates">
            <class>separator-top</class>
            <label>Advanced Checkout License Certificate</label>
            <tab>advanced-checkout</tab>
            <frontend_type>text</frontend_type>
            <sort_order>10000</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <license translate="label">
                    <label>Advanced Checkout License Certificate</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>0</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>0</show_in_website>
                    <show_in_store>0</show_in_store>
                    <fields>		
                        <mwlicense translate="label">
                            <label>Licenses</label>
                            <frontend_type>text</frontend_type>
                            <frontend_model>certificates/config_license</frontend_model>
                            <sort_order>1</sort_order>                            
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <comment></comment>
                        </mwlicense>						                       						
                    </fields>				
                </license>                
            </groups>
        </onestepcheckout>
    </sections>    
</config>